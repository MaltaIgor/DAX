--CALCULO PARA DISTINÇÃO DE STATUS, FÓRMULAS DAX.
1QTD_CONFORMES = CALCULATE(CALCULATE(
		if(isblank(DISTINCTCOUNT(STATUS_CARDS[TECE])),0,DISTINCTCOUNT(STATUS_CARDS[TECE])),
		STATUS_CARDS[DESCRICAO] IN { "Em Conformidade" }
	),ALLSELECTED(STATUS_CARDS[TECE]))





1Percentual CONFORME = 
VAR __MEASURE_VALUE =
	CALCULATE(
		if(isblank(DISTINCTCOUNT(STATUS_CARDS[TECE])),0,DISTINCTCOUNT(STATUS_CARDS[TECE])),
		STATUS_CARDS[DESCRICAO] IN { "Em Conformidade" }
	)
VAR __BASELINE_VALUE = CALCULATE(DISTINCTCOUNT(STATUS_CARDS[TECE]),ALL(STATUS_CARDS[TECE] ))
RETURN
	IF(
		NOT ISBLANK(__MEASURE_VALUE),
		DIVIDE(__MEASURE_VALUE, __BASELINE_VALUE)
	)





1MEDIDA = FIXED(STATUS_CARDS[1QTD_CONFORMES],0,0)
